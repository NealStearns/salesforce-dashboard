  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {"solid": {"color": "#ffffff"}},
            "transparency": 0
          }
        ],
        "border": [
          {
            "color": {"solid": {"color": "#e2e8f0"}},
            "radius": 8
          }
        ],
        "dropShadow": [
          {
            "show": true,
            "color": {"solid": {"color": "#000000"}},
            "position": "Outer",
            "preset": "BottomRight",
            "transparency": 90
          }
        ]
      }
    }
  }
}
